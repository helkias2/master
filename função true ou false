<script>

//--- funcao -----
function setidade(idade){
	return idade;
}
//----------- passando o valor -------
var idade =setidade(30);

//------ verificacao do valor --------
function maioridade(idade){
	
	return(idade >=18);
}
document.getElementById("demo").innerHTML = maioridade(setidade(30)) + " " + setidade(30);

</script>
