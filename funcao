<script>
//---------------------- funcao -----------------
var x = myFunction(5,20);
function myFunction(n1, n2){
	return n1 * n2;
}
//------------------------------------------------

//saida de dados 
document.getElementById("demo").innerHTML = myFunction(5, 20);
//variacel recebendo valor da funcao 
var a = myFunction(5,20);

window.alert("Valor " + a);

</script>
